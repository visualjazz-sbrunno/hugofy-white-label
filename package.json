{
  "name": "isobar-boilerplate-hugo-uikit",
  "version": "1.0.0",
  "description": "A webpack boilerplate for static websites that has all the necessary modern tools and optimizations built-in.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo && npm run algolia",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --minify",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js --hot --inline",
    "algolia": "atomic-algolia"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "keywords": [
    "isobar",
    "boilerplate"
  ],
  "author": "sammyb",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "date-fns": "^1.30.1",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.3",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "hugo-bin": "^0.62.3",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.10.0",
    "netlify-cms-app": "^2.12.19",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^3.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "to-string-loader": "^1.1.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.1",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "algoliasearch": "^4.3.1",
    "atomic-algolia": "^0.3.19",
    "bulma": "^0.9.0",
    "countup.js": "^2.0.6",
    "dotenv-webpack": "^2.0.0",
    "jquery": "^3.5.1",
    "lazysizes": "^4.1.5",
    "lodash": "^4.17.20",
    "react-fast-compare": "^3.2.0",
    "uikit": "^3.5.6"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
